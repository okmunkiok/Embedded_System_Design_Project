<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout 
	xmlns:android="http://schemas.android.com/apk/res/android"   
	android:layout_width="match_parent" 
	android:layout_height="match_parent" 
	android:background="#ffffff">

    <ImageView
        android:id="@+id/image1"
        android:layout_width="270dp"
        android:layout_height="270dp"
        android:layout_above="@+id/image1"
        android:layout_centerHorizontal="true"/>

    <ImageView
        android:id="@+id/image2"
        android:layout_width="270dp"
        android:layout_height="270dp"
        android:layout_above="@+id/button2"
        android:layout_centerHorizontal="true" />
    
    <Button
        android:id="@+id/button1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_marginBottom="16dp"
        android:layout_marginLeft="30dp"
        android:text="Gallery Open"
        android:textSize="18dp" />
    
    <Button
        android:id="@+id/button2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/button1"
        android:layout_alignBottom="@+id/button1"
        android:layout_alignParentRight="true"
        android:layout_marginBottom="16dp"
        android:layout_marginRight="30dp"
        android:text="Binarize(이진화)" />

    <ImageView
        android:id="@+id/ImageView01"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_alignBottom="@+id/image1"/>

    <ImageView
        android:id="@+id/ImageView02"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_alignBottom="@+id/image1"
        android:layout_toRightOf="@+id/ImageView01" />

    <ImageView
        android:id="@+id/ImageView03"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_alignBottom="@+id/image1"
        android:layout_alignRight="@+id/button1" />

    <ImageView
        android:id="@+id/ImageView04"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_alignBottom="@+id/image1"
        android:layout_toRightOf="@+id/ImageView03" />

    <ImageView
        android:id="@+id/ImageView05"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_alignTop="@+id/ImageView04"
        android:layout_toRightOf="@+id/ImageView04" />

    <ImageView
        android:id="@+id/ImageView06"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_alignBottom="@+id/image1"
        android:layout_toRightOf="@+id/ImageView05" />

    <ImageView
        android:id="@+id/ImageView07"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_alignBottom="@+id/image2"
        android:layout_toLeftOf="@+id/ImageView02" />

    <ImageView
        android:id="@+id/ImageView08"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_alignBottom="@+id/image2"
        android:layout_alignLeft="@+id/ImageView02" />

    <ImageView
        android:id="@+id/ImageView09"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_alignTop="@+id/ImageView08"
        android:layout_toRightOf="@+id/ImageView08" />

    <ImageView
        android:id="@+id/ImageView10"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_alignTop="@+id/ImageView09"
        android:layout_toRightOf="@+id/ImageView09" />

    <ImageView
        android:id="@+id/ImageView11"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_alignTop="@+id/ImageView10"
        android:layout_toRightOf="@+id/ImageView10" />

    <ImageView
        android:id="@+id/ImageView12"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_alignTop="@+id/ImageView11"
        android:layout_toRightOf="@+id/ImageView11" />

    <TextView
        android:id="@+id/textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/image1"
        android:layout_marginTop="14dp"
        android:text="글자"
        android:textAppearance="?android:attr/textAppearanceSmall" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView1"
        android:layout_toRightOf="@+id/ImageView01"
        android:text="글자"
        android:textAppearance="?android:attr/textAppearanceSmall" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView2"
        android:layout_alignLeft="@+id/ImageView03"
        android:text="글자"
        android:textAppearance="?android:attr/textAppearanceSmall" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/textView3"
        android:layout_alignBottom="@+id/textView3"
        android:layout_alignLeft="@+id/ImageView04"
        android:text="글자"
        android:textAppearance="?android:attr/textAppearanceSmall" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/textView5"
        android:layout_alignBottom="@+id/textView5"
        android:layout_alignLeft="@+id/ImageView06"
        android:text="글자"
        android:textAppearance="?android:attr/textAppearanceSmall" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/textView4"
        android:layout_alignBottom="@+id/textView4"
        android:layout_alignLeft="@+id/ImageView05"
        android:text="글자"
        android:textAppearance="?android:attr/textAppearanceSmall" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/ImageView07"
        android:layout_alignParentLeft="true"
        android:text="글자"
        android:textAppearance="?android:attr/textAppearanceSmall" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/ImageView08"
        android:layout_alignLeft="@+id/ImageView08"
        android:text="글자"
        android:textAppearance="?android:attr/textAppearanceSmall" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/ImageView09"
        android:layout_alignLeft="@+id/textView3"
        android:text="글자"
        android:textAppearance="?android:attr/textAppearanceSmall" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/ImageView10"
        android:layout_alignLeft="@+id/textView4"
        android:text="글자"
        android:textAppearance="?android:attr/textAppearanceSmall" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/ImageView11"
        android:layout_toRightOf="@+id/ImageView04"
        android:text="글자"
        android:textAppearance="?android:attr/textAppearanceSmall" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/ImageView12"
        android:layout_alignLeft="@+id/textView6"
        android:text="글자"
        android:textAppearance="?android:attr/textAppearanceSmall" />
 
 </RelativeLayout>  
    
